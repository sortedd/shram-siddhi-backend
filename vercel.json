{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/auth/login.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/workers/get-all.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/workers/create.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/client-requests/create.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/contact/create.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/auth/login",
      "dest": "/api/auth/login.js"
    },
    {
      "src": "/api/workers",
      "dest": "/api/workers/get-all.js"
    },
    {
      "src": "/api/workers/create",
      "dest": "/api/workers/create.js"
    },
    {
      "src": "/api/client-requests/create",
      "dest": "/api/client-requests/create.js"
    },
    {
      "src": "/api/contact/create",
      "dest": "/api/contact/create.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
