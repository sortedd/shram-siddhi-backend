{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/auth.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/health",
      "dest": "/api/index.js"
    },
    {
      "src": "/api/auth/login",
      "dest": "/api/auth.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
